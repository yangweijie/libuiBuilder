{
  "name": "libui-builder-preview",
  "version": "1.0.0",
  "description": "libuiBuilder 预览工具 - Formily 版本",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "parser-test": "tsx src/parser/test.ts",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^6.1.0",
    "@formily/core": "^2.3.0",
    "@formily/vue": "^2.3.0",
    "ant-design-vue": "^4.0.0",
    "pinia": "^2.1.0",
    "vue": "^3.4.0",
    "vue-router": "^4.2.0"
  },
  "devDependencies": {
    "@types/jsdom": "^27.0.0",
    "@types/node": "^20.19.27",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "@vitest/ui": "^1.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.0",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.19.0",
    "jsdom": "^24.0.0",
    "less": "^4.4.2",
    "prettier": "^3.1.0",
    "tsx": "^4.7.0",
    "typescript": "~5.3.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.0",
    "vue-tsc": "^2.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
