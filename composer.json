{
  "name": "yangweijie/libui-builder",
  "description": "Builder 方式开发 kingbes/libui gui 应用",
  "type": "library",
  "require": {
    "ext-dom": "*",
    "ext-ffi": "*",
    "ext-libxml": "*",
    "corveda/php-sandbox": "^3.1",
    "kingbes/libui": "*",
    "monolog/monolog": "^3.0@dev"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Kingbes\\Libui\\": "src/",
      "Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "yangweijie",
      "email": "917647288@qq.com"
    }
  ],
  "minimum-stability": "dev",
  "require-dev": {
    "pestphp/pest": "*",
    "phpunit/phpunit": "*"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    },
    "sort-packages": true
  }
}
